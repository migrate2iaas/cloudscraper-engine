HKR, Ndi\params\*LSOv1IPv4,                       Default,    0, "1"
HKR, Ndi\params\*LSOv1IPv4,                       Optional,   0, "0"
HKR, Ndi\params\*LSOv1IPv4\enum,                  "0",        0, %Disabled%
HKR, Ndi\params\*LSOv1IPv4\enum,                  "1",        0, %Enabled%

[Xennet_Service]
